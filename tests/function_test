fun add(a, b, c) {
    println(a + b + c);
}

fun print_hello(name) {
    print("Hello: ");
    println(name);
}

add(1, 2, 3);
add(25, 15, 5);

print_hello("Someone");

fun fib(n) {
    if (n <= 1) return n;
    return fib(n - 2) + fib(n - 1);
}

for (var i = 0; i < 2; i = i + 1) {
    println(fib(i));
}

println("");
println("Clock:");
println(clock()); // build in function
