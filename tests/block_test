var a = 1;
var b = 2;
var c = 3;
{
    var a = 10;
    var b = 20;

    {
        var a = 100;

        println(a); // 100
        println(b); // 20
        println(c); // 3
    }

    println(a); // 10
    println(b); // 20
    println(c); // 3
}

println(a); // 1
println(b); // 2
println(c); // 3
